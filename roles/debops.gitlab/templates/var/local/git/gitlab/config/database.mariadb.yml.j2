#
# PRODUCTION
#
production:
  adapter: mysql2
  encoding: utf8
  reconnect: false
  database: {{ gitlab_database_name }}
  pool: 10
  username: {{ gitlab_database_user }}
  password: "{{ gitlab_database_password }}"
  host: {{ gitlab_database_server }}
  # socket: /tmp/mysql.sock


